#include <stdio.h>
int f[31][31];
int main ()
{
    int i,j,n;
    scanf("%d",&n);
    for(i=1;i<=n;i++)f[i][1]=1;
    for(i=1;i<=n;i++)f[i][i]=1;
    for(i=3;i<=n;i++)
    {
        for(j=2;j<=i-1;j++)
        {
            f[i][j]=f[i-1][j-1]+f[i-1][j];
        }
    }
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=i-1;j++)
        {
            printf("%d ",f[i][j]);
        }
        printf("%d\n",f[i][i]);
    }
    return 0;
}
