int factorsum( int number )
{
	int n=number-1;
	int sum=0;
	if(number==1)
	{
		sum=1;
	}
	for(;n>0;n--)
	{
		if(number%n==0)
		{
			sum+=n;
		}
	}
	return sum;
}

void PrintPN( int m, int n )
{
	int i,cnt=0;
	for(;m<=n;m++)
	{
		if(factorsum(m)==m)
		{
			cnt+=1;
			printf("%d = 1",m);
			for(i=2;i<m;i++)
			{
				if(m%i==0)
				{
					printf(" + %d",i);
				}
			}
			printf("\n");
		}
	}
	if(cnt==0)
	{
		printf("No perfect number");
	}
}
